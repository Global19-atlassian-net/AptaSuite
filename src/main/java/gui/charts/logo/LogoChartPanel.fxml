<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="logoAnchorPane" maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="gui.charts.logo.LogoChartPanelController">
   <children>
      <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <GridPane fx:id="chartGridPane">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="5.0" prefWidth="5.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="5.0" prefHeight="5.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
            <VBox fx:id="noDataVBox" alignment="CENTER" StackPane.alignment="CENTER">
               <children>
                  <Label text="No data to plot found" textFill="#808080">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
